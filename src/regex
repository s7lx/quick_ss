#!/usr/bin/python
#coding: utf8
import re
import sys

def read_file(fn):
    '''read file '''
    fp = open(fn,"r")
    h = fp.read()
    fp.close
    return h

def controller(reg_str,ctstr):
    ''' regx fomula
        content string
         '''
    ct = re.compile(reg_str,re.DOTALL).findall(ctstr)
    for x in ct :
        print x;

    return

if len(sys.argv) < 2 :
    exit()
if len(sys.argv) == 3 :
    f=read_file(sys.argv[2])
else :
    try:
        f =sys.stdin.read()
    except KeyboardInterrupt:
        exit(1)
if len(f) !=0:
    controller(sys.argv[1],f)